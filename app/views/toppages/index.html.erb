<% if logged_in? %>
  <%= render 'tasks/tasks', tasks: @tasks %>
<% else %>
  <div class="center jumbotron">
    <div class="text-center">
      <h1>Task list</h1>
      <%= link_to 'Sign up', signup_path, class: 'btn btn-default btn-lg' %>
      <%= link_to 'Log in', login_path, class: 'btn btn-primary btn-lg' %>
    </div>
  </div>
<% end %>
  